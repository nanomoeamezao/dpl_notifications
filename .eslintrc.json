{
    "plugins": ["prettier"],
    "extends": ["airbnb", "airbnb/hooks", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "script",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "prettier/prettier": "error"
           },
    "env": {
        "node": true,
        "browser": true
    }
}
